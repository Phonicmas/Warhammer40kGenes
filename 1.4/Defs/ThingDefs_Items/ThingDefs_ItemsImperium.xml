<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <!-- Geneseed Capsule -->

  <ThingDef ParentName="ResourceBase">
    <defName>BEWH_GeneseedCapsules</defName>
    <label>geneseed capsule</label>
    <description>A capsule filled with materials needed to augment a baseliner into a space marine.</description>
    <graphicData>
      <texPath>Things/Item/Geneseed</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <tradeability>Buyable</tradeability>
    <stackLimit>50</stackLimit>
    <healthAffectsPrice>false</healthAffectsPrice>
    <statBases>
      <MaxHitPoints>200</MaxHitPoints>
      <MarketValue>13500</MarketValue>
      <Mass>0.6</Mass>
      <Flammability>0</Flammability>
      <DeteriorationRate>0</DeteriorationRate>
    </statBases>
    <intricate>true</intricate>
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
  </ThingDef>

</Defs>
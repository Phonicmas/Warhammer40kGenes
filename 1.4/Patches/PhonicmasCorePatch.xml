<?xml version="1.0" encoding="utf-8"?>
<Patch>

    <Operation Class="PatchOperationConditional">
        <xpath>Defs/WorkGiverDef[defName="BEWH_RitualWorkGiver"]/fixedBillGiverDefs</xpath>
        <nomatch Class="PatchOperationAdd">
            <xpath>Defs/WorkGiverDef[defName="BEWH_RitualWorkGiver"]</xpath>
            <value>
                <fixedBillGiverDefs />
            </value>
        </nomatch>
    </Operation>


    <Operation Class="PatchOperationAdd">
        <xpath>Defs/WorkGiverDef[defName = "BEWH_RitualWorkGiver"]/fixedBillGiverDefs</xpath>
        <value>
            <li>BEWH_UndividedRitualSpot</li>
            <li>BEWH_KhorneRitualSpot</li>
            <li>BEWH_TzeentchRitualSpot</li>
            <li>BEWH_NurgleRitualSpot</li>
            <li>BEWH_SlaaneshRitualSpot</li>
            <li>BEWH_ImperiumSpot</li>
        </value>
    </Operation>

</Patch>